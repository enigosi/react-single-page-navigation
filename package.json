{
  "name": "react-single-page-navigation",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "example:serve": "parcel example/index.html --open",
    "example:build": "parcel build example/index.html -d dist/example",
    "example:deploy": "yarn example:build && gh-pages -d dist/example",
    "test": "jest",
    "lint": "tslint --project tsconfig.json"
  },
  "author": "Wojciech Romanski",
  "license": "ISC",
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jsdom": "^13.1.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn lint"
    }
  }

}
